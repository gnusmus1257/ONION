@model List<SimilarViewModel>



<environment include="Development">
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
</environment>


@foreach (var person in Model)
{
    <a target="_blank" href="/Home/GetArtist?name=@person.Name">
        <div class="col-md-2" style="height: 350px; margin: 0px; padding: 0px;">
            <img src="@person.Photo" style="width: 100%" />
            <h4 class="text-center">@person.Name</h4>
        </div>
    </a>

}